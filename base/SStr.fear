package base;

Str:Sealed,DataType[Str]{
  .asDataType->::;
  +(x: ToStr): Str;
  |(x: ToStr): Str;          // like + but inserts newline between
  ^(x: ToStr): Str;        // concat with backticks around x.str
  ^^(x: ToStr): Str;       // concat with double quotes around x.str
  |:Str;//adds \n
  ^:Str;//adds `
  ^^:Str;//adds "
  .hash(h) -> h.str(this.imm);
